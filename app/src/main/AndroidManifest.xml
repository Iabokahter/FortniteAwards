<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sevenhills.fortniteawards">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".AboutUs_Activity"></activity>
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".InviteFriendActivity"></activity>
        <activity android:name=".SupportActivity"></activity>
        <activity android:name=".AccountActivity"></activity>
        <activity android:name=".ThemeActivity"></activity>
        <activity android:name=".TopOneActivity"></activity>
        <activity android:name=".MotherActivity"></activity>
        <activity android:name=".WalletActivity"></activity>
        <activity android:name=".Sign_Up_Activity"></activity>
        <activity android:name=".SignIn_Activity"></activity>
    </application>

</manifest>